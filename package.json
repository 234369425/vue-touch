{
  "name": "vue-touch",
  "version": "2.0.0-beta.1",
  "main": "dist/vue-touch.js",
  "files": [
    "dist/vue-touch.js",
    "dist/vue-touch.min.js"
  ],
  "description": "Hammer.js based touch events plugin for Vue.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuejs/vue-touch.git"
  },
  "bugs": "https://github.com/vuejs/vue-touch/issues",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "browser-env": "^2.0.19",
    "buble": "^0.15.1",
    "buble-loader": "^0.3.2",
    "css-loader": "^0.26.1",
    "hammerjs": "^2.0.8",
    "inspect-process": "^0.4.0",
    "rollup": "^0.37.0",
    "rollup-plugin-buble": "^0.15.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.6.2",
    "vue": "^2.1.6",
    "webpack": "^2.1.0-beta.28"
  },
  "peerDependecies": {
    "vue": "^2.0.0",
    "hammerjs": "^2.0.8"
  },
  "scripts": {
    "build": "node_modules/.bin/rollup -c build/rollup.config.prod.js && uglifyjs dist/vue-touch.js -c -m > dist/vue-touch.min.js",
    "watch": "webpack --watch --config build/webpack.config.dev.js",
    "example": "browser-sync start --config bs-config.js",
    "test": "NODE_ENV=development node_modules/.bin/ava --verbose",
    "test:prod": "NODE_ENV=production npm run build && node_modules/.bin/ava --verbose",
    "test:watch": "NODE_ENV=development node_modules/.bin/ava --watch --verbose",
    "test:debug": "NODE_ENV=development inspect-process node_modules/ava/profile.js test/index.js"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/browser-env.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015-node6",
      "stage-2"
    ]
  },
  "dependencies": {
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0"
  }
}
